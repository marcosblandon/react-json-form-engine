{
  "name": "react-json-form-engine",
  "version": "0.2.29",
  "author": "Mike Chabot",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "republish": "npm run build && npm run auto-commit && npm version patch && npm publish",
    "css-build": "npm run css-build-nested && npm run css-build-compressed",
    "css-build-nested": "node-sass --omit-source-map-url --output-style nested src/sass/react-json-form-engine.scss dist/css/react-json-form-engine.css",
    "css-build-compressed": "node-sass --omit-source-map-url --output-style compressed src/sass/react-json-form-engine.scss dist/css/react-json-form-engine.min.css",
    "build": "rm -rf ./dist && npm run css-build && babel src -d dist",
    "auto-commit": "git add . && git commit -m \"Packaging assets\"",
    "storybook": "npm run build && start-storybook -p 6006",
    "build-storybook": "npm run build && build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikechabot/react-json-form-engine.git"
  },
  "keywords": [
    "react",
    "forms",
    "json forms",
    "conditional forms"
  ],
  "bugs": {
    "url": "https://github.com/mikechabot/react-json-form-engine/issues"
  },
  "homepage": "http://mikechabot.github.io/react-json-form-engine-storybook",
  "peerDependencies": {
    "bulma": "^0.7.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.0.5",
    "@storybook/addon-knobs": "^5.0.5",
    "@storybook/addon-links": "^5.0.5",
    "@storybook/addon-options": "^5.0.5",
    "@storybook/addons": "^5.0.5",
    "@storybook/react": "^5.0.5",
    "babel-loader": "^8.0.5",
    "bulma": "^0.7.4",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "why-did-you-update": "^1.0.6"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-regular-svg-icons": "^5.8.1",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "api-check": "^7.5.5",
    "core-js": "^3.0.0",
    "glamor": "^2.20.40",
    "glamorous": "^4.13.0",
    "lodash": "^4.17.11",
    "maybe-baby": "^1.1.8",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "16.8.6",
    "react-datepicker": "^2.3.0",
    "react-dom": "16.8.6",
    "react-tabify": "^0.1.24",
    "sortable-map": "0.0.17"
  }
}
